view {
  font-size: 0.7rem;
}
.container{
  background: #e7edf2;
}
.sub-container{
  width: 100%;
}
.block{
  background: #fff;
  margin: 30rpx 0 0;
  width: 100%;
}
.section{
  padding: 20rpx 0;
  margin: 0 40rpx;
}
.section::after{
  content: '';
  display: block;
  clear: both;
}
.section-line{
  border-bottom: 1px solid #f1f1f1;
}
.title{
  float: left;
  margin: auto 20px auto 0;
  height: 1.4rem;
  line-height: 1.4rem;
}
.input-container{
  float: right;
  height: 1.4rem;
  line-height: 1.4rem;
}
.text {
  float:left;
  text-align: right;
  color: #999999;
}
.icon{
  background-image: url("data:image/svg+xml;base64,");
  margin-left: 20rpx;
  float: right;
  height: 1.4rem;
  line-height: 1.4rem;
  width: 1.4rem;
  background-size: 1.4rem 1.4rem;
}
.btn-view{
  width: 100%;
  border-radio: 0;
  margin-bottom: 100px;
}
button{
  border-radius: 0;
  border: none;
  background: #009bff;
  color: #fff;
}