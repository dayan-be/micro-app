}

.rule .item .btn .text {
  background: rgba(249, 182, 108, 1);
  border-radius: 15px;
  font-size: 12px;
  padding: 6px 18px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 17px;
}

.common-block .body .last-item {
  margin-right: 20px;
}

.common-block .footer {
  font-size: 12px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #cdb9ad;
  line-height: 22px;
  padding: 10px;
}

.slogan {
  margin-bottom: 20px;
  text-align: center;
  color: #eee;
}
